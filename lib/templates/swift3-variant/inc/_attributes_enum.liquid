
{% if entity.attributes.size > 0 %}
  enum Attributes: String {
    {%- for attribute in entity.attributes -%}
        {%- if attribute.realm_ignored == false -%}
            {%- if attribute.comment.size > 0 %}
    /// {{ attribute.comment }}
            {%- endif %}
    case {{ attribute.name }} = "{{ attribute.name | escape_quotes }}"
        {%- endif %}
    {%- endfor %}
  }
{% endif -%}