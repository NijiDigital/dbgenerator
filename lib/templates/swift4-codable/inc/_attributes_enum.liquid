  {% if params.public %}public {% endif %}private enum Attributes: String, CodingKey {
    {%- for attribute in entity.attributes -%}
        {% assign attributeKey = attribute.name -%}
        {%- if attribute.json_key_path.size > 0 -%}
          {%- assign attributeKey = attribute.json_key_path -%}
        {%- endif -%}
        {%- if attribute.realm_ignored or attribute.realm_read_only.size == 0 -%}
            {%- if attribute.comment.size > 0 %}
    /** {{ attribute.comment }} **/
            {%- endif %}
    case {{ attribute.name }} = "{{ attributeKey | escape_quotes }}"{% if attributeKey == entity.identity_attribute %} /* Primary Key */{% endif %}
        {%- endif %}
    {%- endfor %}
  }
{% endif -%}
